<?xml version="1.0"?>
<classes>

  <!-- EXTENSIONS OF CORE CLASSES -->
  
  <!-- sources/fasta source has been updated to set these attributes -->
  <class name="BioEntity" is-interface="true">
    <attribute name="assemblyVersion" type="java.lang.String"/>
    <attribute name="annotationVersion" type="java.lang.String"/>
    <attribute name="description" type="java.lang.String"/>
  </class>

  <!-- required by create-intergenic-region-features and create-gene-flanking-features post-processes -->
  <class name="Gene" is-interface="true">
    <reference name="upstreamIntergenicRegion" referenced-type="IntergenicRegion" term="http://purl.obolibrary.org/obo/SO_0000605"/>
    <reference name="downstreamIntergenicRegion" referenced-type="IntergenicRegion" term="http://purl.obolibrary.org/obo/SO_0000605"/>
    <collection name="flankingRegions" referenced-type="GeneFlankingRegion" reverse-reference="gene" term="http://purl.obolibrary.org/obo/SO_0000239"/>
  </class>

  <!-- LIS enforces one transcript per protein, so override the reverse-reference here -->
  <class name="Transcript" is-interface="true">
    <reference name="protein" referenced-type="Protein" reverse-reference="transcript" term="http://purl.uniprot.org/core/Protein,http://semanticscience.org/resource/SIO_010043"/>
  </class>

  <!-- LIS enforces one transcript per protein, a deviation from the SO, which has Protein.transcripts -->
  <class name="Protein" is-interface="true">
    <reference name="transcript" referenced-type="Transcript" reverse-reference="protein" term="http://purl.obolibrary.org/obo/SO_0000673"/>
  </class>

  <!-- NEW CLASSES -->

  <!-- required by go source -->
  <class name="GOTerm" extends="OntologyTerm" is-interface="true"/>

  <!-- required by create-gene-flanking-features post-process -->
  <class name="GeneFlankingRegion" extends="SequenceFeature" is-interface="true" term="http://purl.obolibrary.org/obo/SO_0000239">
    <attribute name="distance" type="java.lang.String" term="http://purl.obolibrary.org/obo/PATO_0000040"/>
    <attribute name="direction" type="java.lang.String" term="http://purl.obolibrary.org/obo/PATO_0000039"/>
    <attribute name="includeGene" type="java.lang.Boolean"/>
    <reference name="gene" referenced-type="Gene" reverse-reference="flankingRegions" term="http://purl.obolibrary.org/obo/SO:0000704"/>
  </class>

  <!-- required by create-intergenic-region-features post-process -->
  <class name="IntergenicRegion" extends="SequenceFeature" is-interface="true" term="http://purl.obolibrary.org/obo/SO_0000605">
    <collection name="adjacentGenes" referenced-type="Gene"/>
  </class>

  <!-- NOTE: interpro loader modified to have ProteinDomain extend Annotatable -->
  <class name="ProteinDomain" extends="Annotatable" is-interface="true" term="http://purl.obolibrary.org/obo/SO:0000417">
    <attribute name="description" type="java.lang.String"/>
  </class>

</classes>
